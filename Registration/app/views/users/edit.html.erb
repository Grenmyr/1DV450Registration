<% provide(:title, "Edit user") %>
<div class="center jumbotron " >
<h1>Update your profile</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
        <%= render 'upsert', f: f %>

        <%= f.submit "Update my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
</div>
